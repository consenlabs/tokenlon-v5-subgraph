    "prepare:mainnet": "mustache config/mainnet.json template.yaml > limitorder.yaml",
    "prepare:arbitrum": "mustache config/arbitrum.json template.yaml > limitorder.yaml",
    "prepare:goerli": "mustache config/goerli.json template.yaml > limitorder.yaml"
